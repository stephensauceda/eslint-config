{
  "name": "eslint-config-stephensauceda",
  "description": "An ESLint config for my personal projects",
  "version": "0.0.0-semantically-released",
  "main": "index.js",
  "scripts": {
    "test": "eslint --fix index.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stephensauceda/eslint-config-stephensauceda.git"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "config",
    "javascript",
    "stephensauceda"
  ],
  "author": "Stephen Sauceda <contact@stephensauceda.com> (http://stephensauceda.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stephensauceda/eslint-config-stephensauceda/issues"
  },
  "homepage": "https://github.com/stephensauceda/eslint-config-stephensauceda#readme",
  "peerDependencies": {
    "eslint": "^2.0.0"
  },
  "devDependencies": {
    "commitizen": "^2.3.0",
    "cz-conventional-changelog": "1.1.4",
    "eslint": "^1.8.0",
    "ghooks": "^0.3.2",
    "semantic-release": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test"
    }
  },
  "dependencies": {
    "babel-eslint": "^4.1.8",
    "eslint-config-airbnb": "^5.0.1",
    "eslint-plugin-react": "^3.16.1"
  }
}
